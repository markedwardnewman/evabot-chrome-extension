{
  "name": "Chrome Extension - Evabot",
  "description": "A delightful way to reward your team with real gifts",
  "version": "1.0.0",  
  "permissions": [
    "webNavigation",
    "*://*.google.com/*",
    "storage"
   ],
  "browser_action": {
    "default_icon": "fab.png",
    "default_title": "Evabot",
    "default_popup": "evabot.html"
  },
  "manifest_version": 2
}